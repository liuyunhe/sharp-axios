{
  "include": [
    "src/index.ts",
    "src/axios.ts",
    "src/types/*",
    "src/helpers/*",
    "src/core/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "lib": ["ESNext", "DOM", "DOM.Iterable", "ScriptHost"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "references": [
    {
      "path": "./tsconfig.json"
    }
  ]
}
